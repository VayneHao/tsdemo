{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "target": "es5",
    "lib": [
      "dom",
      "es7",
      "es5",
      "es2015"
    ]
  },
  "exclude": [
    "node_modules",
    "lib",
    "es"
  ]
}